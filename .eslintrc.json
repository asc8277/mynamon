{
    "env": {
        "es2021": true,
        "browser": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "globals": {
        "io": true
    },
    "rules": {
        "semi": ["error", "always"]
    }
}
